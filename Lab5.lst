0000            103   ; For the DE1-SoC configured with an 8052 softcore processor
0000            104   ; we have the following extra registers:
0000            105   
0000            106   HEX0   DATA  091H ; Zero turns the segment on
0000            107   HEX1   DATA  092H ; 
0000            108   HEX2   DATA  093H ; 
0000            109   HEX3   DATA  094H ; 
0000            110   HEX4   DATA  08EH ;
0000            111   HEX5   DATA  08FH ;
0000            112   
0000            113   P0MOD  DATA  09AH ; Input/output mode bits for port 0.  '1' sets the port to output mode.
0000            114   P1MOD  DATA  09BH ; Input/output mode bits for port 1
0000            115   P2MOD  DATA  09CH ; Input/output mode bits for port 2
0000            116   P3MOD  DATA  09DH ; Input/output mode bits for port 3
0000            117   
0000            118   LEDRA  DATA  0E8H ; LEDs LEDR0 to LEDR7 (bit addressable, ex: LEDRA.1 for LEDR1)
0000            119   LEDRB  DATA  095H ; LEDs LEDR8 to LEDR9
0000            120   SWA    DATA  0E8H ; Switches SW0 to SW7 (bit addressable, ex: SWA.1 for SW1)
0000            121   SWB    DATA  095H ; Switches SW8 to SW9
0000            122   KEY    DATA  0F8H ; KEY1=KEY.1, KEY2=KEY.2, KEY3=KEY.3.  KEY0 is the reset button! 
0000            123   
0000            124   LCD_CMD   DATA 0D8H ;
0000            125   LCD_DATA  DATA 0D9H ;
0000            126   LCD_MOD   DATA 0DAH ; Write 0xff to make LCD_DATA an output
0000            127   LCD_RW    BIT  0D8H ; '0' writes to LCD
0000            128   LCD_EN    BIT  0D9H ; Toggle from '1' to '0'
0000            129   LCD_RS    BIT  0DAH ; '0' for commands, '1' for data
0000            130   LCD_ON    BIT  0DBH ; Write '1' to power the LCD
0000            131   LCD_BLON  BIT  0DCH ; Write '1' to turn on back light
0000            132   
0000            133   FLASH_CMD  data 0DBH ; The control bits of the flash memory:
0000            134   ; bit 0: FL_RST_N  Set to 1 for normal operation
0000            135   ; bit 1: FL_WE_N
0000            136   ; bit 2: FL_OE_N
0000            137   ; bit 3: FL_CE_N
0000            138   FLASH_DATA data 0DCH ; 8-bit data bus of flash memory.
0000            139   FLASH_MOD  data 0DDH ; 0xff makes FLASH_DATA output.  0x00 makes FLASH_DATA input.
0000            140   FLASH_ADD0 data 0E1H ; address bits 0 to 7.
0000            141   FLASH_ADD1 data 0E2H ; address bits 8 to 15.
0000            142   FLASH_ADD2 data 0E3H ; address bits 16 to 21.
